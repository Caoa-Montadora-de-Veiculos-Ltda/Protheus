User Function MT110COR

Local aLocCor 	:= PARAMIXB[1]
Local i			:= 0

aAdd(aLocCor,{'C1_RESIDUO=="S" .AND. C1_XCPARCE=="S" '   		, 'BR_AZUL_CLARO'})

For i:=1 to Len(aLocCor)
	If aLocCor[i,2]=="BR_PRETO"
		aLocCor[i,1] := '!Empty(C1_RESIDUO) .AND. Empty(C1_XCPARCE)' 
	ElseIF aLocCor[i,2]=="ENABLE" 
		aLocCor[i,1] := aLocCor[i,1]+' .AND. Empty(C1_XCPARCE)' 
	Endif
Next

Return(aLocCor)
